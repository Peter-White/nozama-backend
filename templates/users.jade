extend layout
block main
  h1 Users
  .users
  for user in users
    h3 Accout Details:
    p #{user.firstName} #{user.lastName}
    p #{user.emailAddress}
    p #{user.phoneNumber}
    for address in user.address
      p #{address.addressType}:
      p #{address.street} #{address.secondStreet},
      p #{address.city}, #{address.state}, #{address.zipCode}
      p #{address.country}

